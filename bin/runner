#!/usr/bin/env ruby
require 'bundler/setup'
require_relative "../lib/unscaled"

CONFIGS = {
  "foo" => {
    processes: {
      1 => {id: 1, command: 'ls /'},
      2 => {id: 2, command: 'python -m SimpleHTTPServer 8000'}
    }
  }
}

process_list = Unscaled::ProcessList.new(CONFIGS["foo"][:processes])

require 'pry'; binding.pry
